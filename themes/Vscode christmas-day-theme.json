{
  "name": "Christmas Day",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "editor.background": "#fafafa",
    "editor.foreground": "#2d2d2d",
    "sideBar.background": "#f5f5f5",
    "activityBar.background": "#eeeeee",
    "panel.background": "#f5f5f5",
    "editorLineNumber.foreground": "#999999",
    "editorLineNumber.activeForeground": "#bb2528",
    "editorCursor.foreground": "#ea4630",
    "editor.selectionBackground": "#f8b22944",
    "editor.lineHighlightBackground": "#f8b22911",
    "editorWhitespace.foreground": "#dddddd",
    "editorIndentGuide.background1": "#e0e0e0",
    "editorIndentGuide.activeBackground1": "#c0c0c0",
    "editorOverviewRuler.border": "#00000000",
    "editorGutter.addedBackground": "#146b3a",
    "editorGutter.modifiedBackground": "#f8b229",
    "editorGutter.deletedBackground": "#bb2528",
    "textLink.foreground": "#146b3a",
    "textLink.activeForeground": "#165b33",
    "input.background": "#ffffff",
    "input.foreground": "#2d2d2d",
    "input.border": "#dddddd",
    "dropdown.background": "#ffffff",
    "dropdown.border": "#dddddd",
    "editorWidget.background": "#ffffff",
    "editorWidget.border": "#dddddd",
    "editorSuggestWidget.background": "#ffffff",
    "editorSuggestWidget.border": "#dddddd",
    "editorHoverWidget.background": "#ffffff",
    "editorHoverWidget.border": "#dddddd",
    "tab.activeBackground": "#fafafa",
    "tab.inactiveBackground": "#eeeeee",
    "tab.activeForeground": "#2d2d2d",
    "tab.inactiveForeground": "#666666",
    "tab.border": "#e0e0e0",
    "statusBar.background": "#eeeeee",
    "statusBar.foreground": "#2d2d2d",
    "focusBorder": "#f8b229",
    "editorBracketMatch.background": "#f8b22933",
    "editorBracketMatch.border": "#f8b229"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#888888", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": { "foreground": "#165b33" }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "support.constant",
        "keyword.other.unit"
      ],
      "settings": { "foreground": "#d89400" }
    },
    {
      "name": "Punctuation / Operators",
      "scope": ["punctuation", "keyword.operator", "punctuation.definition"],
      "settings": { "foreground": "#555555" }
    },
    {
      "name": "Invalid / Errors",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff6058" }
    },
    {
      "name": "JS Keywords (Christmas Green)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.ternary"
      ],
      "settings": { "foreground": "#146b3a", "fontStyle": "bold" }
    },
    {
      "name": "Built-in / Globals (Gold)",
      "scope": [
        "support.class.builtin",
        "support.type.builtin",
        "support.function",
        "support.function.builtin",
        "support.variable",
        "variable.language",
        "constant.language"
      ],
      "settings": { "foreground": "#d89400" }
    },
    {
      "name": "Function Names (Custom) (Firebrick Red)",
      "scope": [
        "entity.name.function",
        "meta.function entity.name.function",
        "meta.function-call entity.name.function",
        "entity.name.function.js",
        "entity.name.function.ts",
        "entity.name.function.jsx",
        "entity.name.function.tsx"
      ],
      "settings": { "foreground": "#bb2528", "fontStyle": "bold" }
    },
    {
      "name": "Method Calls / Member Functions (Purple)",
      "scope": [
        "meta.method-call",
        "meta.function-call",
        "variable.function",
        "entity.name.method",
        "entity.name.method.js",
        "entity.name.method.ts"
      ],
      "settings": { "foreground": "#8b5fd6" }
    },
    {
      "name": "Variables (General)",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.other.constant"
      ],
      "settings": { "foreground": "#2d2d2d" }
    },
    {
      "name": "Parameters (Gold)",
      "scope": ["variable.parameter", "meta.function.parameters variable"],
      "settings": { "foreground": "#d89400" }
    },
    {
      "name": "Object Properties (Purple)",
      "scope": [
        "variable.other.property",
        "meta.object-literal.key",
        "support.type.property-name",
        "meta.member.expression variable.other.property"
      ],
      "settings": { "foreground": "#8b5fd6" }
    },
    {
      "name": "Classes / Types (Cinnabar Red)",
      "scope": [
        "entity.name.type.class",
        "entity.name.class",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace"
      ],
      "settings": { "foreground": "#ea4630", "fontStyle": "bold" }
    },
    {
      "name": "HTML Content Text",
      "scope": ["text.html.basic"],
      "settings": { "foreground": "#2d2d2d" }
    },
    {
      "name": "HTML Tag Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": { "foreground": "#888888" }
    },
    {
      "name": "HTML Tag Names (Firebrick Red)",
      "scope": ["entity.name.tag.html"],
      "settings": { "foreground": "#bb2528", "fontStyle": "bold" }
    },
    {
      "name": "HTML Attribute Names (Dark Green)",
      "scope": ["entity.other.attribute-name.html"],
      "settings": { "foreground": "#146b3a" }
    },
    {
      "name": "HTML Attribute Values",
      "scope": ["string.quoted.double.html", "string.quoted.single.html"],
      "settings": { "foreground": "#165b33" }
    },
    {
      "name": "HTML Class Attribute Values",
      "scope": [
        "meta.attribute.class.html string.quoted.double.html",
        "meta.attribute.class.html string.quoted.single.html"
      ],
      "settings": { "foreground": "#146b3a" }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": { "foreground": "#ea4630" }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.sass",
        "support.type.property-name.less"
      ],
      "settings": { "foreground": "#8b5fd6" }
    },
    {
      "name": "CSS Values & Units",
      "scope": [
        "constant.numeric.css",
        "keyword.other.unit.css",
        "support.constant.color.css",
        "constant.other.color.hex.css",
        "constant.other.color.rgb-value.css",
        "constant.other.color.hsl-value.css"
      ],
      "settings": { "foreground": "#d89400" }
    },
    {
      "name": "CSS At-rules",
      "scope": ["keyword.control.at-rule.css", "keyword.control.at-rule.scss"],
      "settings": { "foreground": "#146b3a", "fontStyle": "bold" }
    },
    {
      "name": "Markup - Headings",
      "scope": [
        "markdown.heading",
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": { "foreground": "#bb2528", "fontStyle": "bold" }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": { "foreground": "#ea4630", "fontStyle": "bold" }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": { "foreground": "#146b3a", "fontStyle": "italic" }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote",
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": { "foreground": "#666666", "fontStyle": "italic" }
    },
    {
      "name": "Markup - Raw",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.raw.block",
        "markup.fenced_code.block.markdown",
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": { "foreground": "#d89400" }
    },
    {
      "name": "Links",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": { "fontStyle": "underline", "foreground": "#146b3a" }
    }
  ]
}
